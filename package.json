{
	"name": "dash-compiler",
	"version": "0.8.21",
	"description": "A fast compiler for Minecraft Add-Ons",
	"scripts": {
		"build:types": "tsc --project tsconfig.json",
		"build:only": "vite build && vite --config vite.config.skypack.js build",
		"build": "npm run build:only && npm run build:types"
	},
	"main": "./dist/dash-compiler.umd.js",
	"module": "./dist/dash-compiler.es.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/dash-compiler.es.js",
			"require": "./dist/dash-compiler.umd.js"
		},
		"./bundled": {
			"import": "./dist/dash-compiler.bundled.es.js",
			"require": "./dist/dash-compiler.bundled.umd.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bridge-core/dash-compiler.git"
	},
	"author": "solvedDev",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bridge-core/dash-compiler/issues"
	},
	"homepage": "https://github.com/bridge-core/dash-compiler#readme",
	"dependencies": {
		"bridge-common-utils": "^0.3.0",
		"is-glob": "^4.0.3",
		"json5": "^2.2.0",
		"mc-project-core": "^0.3.11",
		"micromatch": "^4.0.4",
		"molang": "^1.13.10",
		"path-browserify": "^1.0.1",
		"typescript": "^4.5.4"
	},
	"devDependencies": {
		"@types/is-glob": "^4.0.2",
		"@types/node": "^16.11.12",
		"@types/path-browserify": "^1.0.0",
		"vite": "^2.6.13"
	}
}
